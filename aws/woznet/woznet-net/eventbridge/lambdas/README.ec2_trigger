# zip it from the working directory and write it to this dir. I think -r means recursive.
rm ec2_trigger.zip
cd ec2_trigger
zip -r ../ec2_trigger.zip .
cd ..

# If the lambda is already deployed, you can just update the code
aws lambda update-function-code --function-name ec2_trigger --zip-file fileb://ec2_trigger.zip --profile ourzoo-root --region us-east-1

