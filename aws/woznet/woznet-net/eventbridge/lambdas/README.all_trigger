# zip it from the working directory and write it to this dir. I think -r means recursive.
rm all_trigger.zip
cd all_trigger
zip -r ../all_trigger.zip .
cd ..

# If the lambda is already deployed, you can just update the code
aws lambda update-function-code --function-name all_trigger --zip-file fileb://all_trigger.zip --profile ourzoo-root --region us-east-1

