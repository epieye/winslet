# zip it from the working directory and write it to this dir. I think -r means recursive.
rm auto_trigger.zip
cd auto_trigger
zip -r ../auto_trigger.zip .
cd ..

# If the lambda is already deployed, you can just update the code
aws lambda update-function-code --function-name auto_trigger --zip-file fileb://auto_trigger.zip --profile ourzoo-root --region us-east-1/


